{
  "name": "adminator",
  "version": "2.0.4",
  "private": true,
  "description": "HTML Admin Template",
  "scripts": {
    "start": "webpack server",
    "dev": "webpack-dashboard -t 'Project' -- webpack server",
    "clean": "shx rm -rf ./dist",
